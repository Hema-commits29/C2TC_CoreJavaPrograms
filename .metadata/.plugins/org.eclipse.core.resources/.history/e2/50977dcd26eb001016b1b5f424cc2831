n
package collections2;

import java.util.Collections;
import java.util.HashSet;
import java.util.Spliterator;

public class HashSetRef {
	//private static final String Set1 = null;

	static void addElements(HashSet<Integer> Set1) {
		Set1.add(17);
		Set1.add(41);
		Set1.add(91);
		Set1.add(51);
		Set1.add(80);
		Set1.add(11);
		Set1.add(20);
		System.out.println(Set1.add(51)); //returns false - not allow to insert duplicate element
	}

	public static void main(String[] args) {
		
		HashSet<Integer> Set1 = new HashSet<Integer>();
		addElements(Set1);

		HashSet<Integer> numberHashSet2 = new HashSet<Integer>();
		numberHashSet2.add(20);
		numberHashSet2.add(80);
		numberHashSet2.add(11);
		numberHashSet2.add(50);
		numberHashSet2.add(60);
		numberHashSet2.add(10);
		numberHashSet2.add(51);

		System.out.println("Set 1: " + Set1);
		System.out.println("Set 2: " + Set1);

		HashSet<Integer> numberHashSet3 = new HashSet<Integer>();
		addElements(numberHashSet3);

		numberHashSet3.addAll(Set1);
		System.out.println("Resultant Set : " + numberHashSet3);

		numberHashSet3.clear();
		addElements(numberHashSet3);
	
		numberHashSet3.retainAll(numberHashSet2);
		System.out.println("Resultant Set : " + numberHashSet3);

		numberHashSet3.clear();
		addElements(numberHashSet3);

		numberHashSet3.removeAll(Set1);
		System.out.println("Resultant Set : " + numberHashSet3);

		Spliterator<Integer> split= Set1.spliterator();
		split.forEachRemaining(System.out::println);
	}

}