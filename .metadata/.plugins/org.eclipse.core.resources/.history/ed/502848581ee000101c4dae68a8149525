package Polymorphism;

class Payment {
	void pay() {
		System.out.println("The payment is done");
	}
}

class gpay extends Payment{
	@Override
	void pay() {
		System.out.println("The payment is done by gpay.");
	}
}


public class MethodOverriding {

	public static void main(String[] args) {
		
		Payment p = new gpay();
		p.pay();
		

	}

}
