package Polymorphism;

class Calculator{
	
	
	int add(int a, int b) {
		return a+b;
	}
	
	int add(int a, int b, int c) {
		return a+b+c;
	}
	
	double add(double a, double b) {
		return a+b;
	}

}

public class MethodOverloading {

	public static void main(String[] args) {
		
		Calculator c = new Calculator();
		
		System.out.println("The value is :" +c.add(10, 15));
		System.out.println("The value is :" +c.add(10, 15, 20));
		System.out.println("The value is :" +c.add(7.5, 9.5));
	
	

	}

}
